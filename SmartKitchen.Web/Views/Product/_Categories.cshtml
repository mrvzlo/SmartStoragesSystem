@model List<SmartKitchen.Domain.DisplayModels.CategoryDisplay>

<script>
    var select = [];
    var opt;
    @foreach (var i in Model)
    {
        <text>
    opt = document.createElement('option');
    opt.value = @i.Id;
    opt.innerHTML = "@i.Name";
    select.push(opt);
        </text>
    }
</script>